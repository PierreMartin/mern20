  "scripts": {
    "build": "npm run clean && npm-run-all --parallel server client:prod -- --env.server",
    "dev": "npm run client:dev -- --env.client",
    "dev:server": "npm-run-all --parallel server:hot client:dev -- --env.server",
    "server": "node index.js",
    "server:hot": "nodemon index.js",
    "client:dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --progress --colors --display-error-details --config configs/webpack.dev.config.js",
    "client:prod": "cross-env NODE_ENV=production webpack --progress --config configs/webpack.prod.config.js",
    "clean": "rimraf dist"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
